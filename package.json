{
  "name": "stage-selected",
  "displayName": "Stage Selected",
  "description": "Adds a \"stage selected\" command to the editor context menu.",
  "version": "0.0.1",
  "publisher": "ch4mb3rs",
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/camchambers/stage-selected"},
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "SCM Providers"
  ],
  "keywords": [
    "git",
    "stage",
    "staging",
    "partial",
    "selection",
    "source control",
    "vcs"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "stage-selected.stageSelected",
          "group": "navigation",
          "when": "editorTextFocus && editorHasSelection && gitOpenRepositoryCount != 0"
        }
      ]
    },
    "commands": [
      {
        "command": "stage-selected.stageSelected",
        "title": "Stage Selected"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "ts-loader": "^9.4.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1",
    "@vscode/test-electron": "^2.3.2"
  }
}
